{
    "name": "contao/notifier-bundle",
    "type": "contao-bundle",
    "description": "Adds notifier functionality to Contao",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "self.version",
        "symfony/http-kernel": "4.4.* || 5.2.*"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "symfony/http-client": "4.4.* || 5.2.*"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\NotifierBundle\\ContaoManager\\Plugin",
        "symfony": {
            "require": "4.4.* || ^5.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Contao\\NotifierBundle\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/contao/contao/issues",
        "forum": "https://community.contao.org",
        "source": "https://github.com/contao/notifier-bundle",
        "docs": "https://docs.contao.org"
    }
}
